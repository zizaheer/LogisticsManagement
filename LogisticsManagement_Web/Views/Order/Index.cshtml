                        @*<div class="col-sm-2 form-group">
                                    <label for="txtPassOnEmployeeNameForDeliverModal">Pass-on to</label>
                                    <input type="text" id="txtPassOnEmployeeNameForDeliverModal" class="form-control form-control-sm txtPassOnEmployeeNameForDeliverModal" readonly />
