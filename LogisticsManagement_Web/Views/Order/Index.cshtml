@using LogisticsManagement_Web.Models;
@model ViewModel_DeliveryOrder


@{
    ViewData["Title"] = "DISPATCH BOARD";
}

<style>
    

    
</style>
<link href="https://cdn.jsdelivr.net/gh/gitbrent/bootstrap4-toggle@3.4.0/css/bootstrap4-toggle.min.css" rel="stylesheet">

<div class="modal fade" id="orderPickup" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content bg-light">
            <div class="modal-header">
                <label class="modal-title"> PICKUP </label>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form class="" style="text-transform:uppercase; font-weight:600">
                <div class="modal-body">
                    <div class="row mb-3">
                        <div class="form-group col-sm-2">
                            <label for="txtWayBillNoForPickupModal">W/B #</label>
                            <input type="hidden" id="hfOrderIdForOrderStatusUpdate" />
                            <input class="form-control form-control-sm frmfontsize" placeholder="W/B #" id="txtWayBillNoForPickupModal" type="number" name="txtWayBillNoForPickupModal" readonly>
                        </div>
                        <div class="form-group col-sm-4">
                            <label for="txtDispatchDateTimeForPickupModal">Dispatch date and time</label>
                            <input type="datetime-local" id="txtDispatchDateTimeForPickupModal" class="form-control form-control-sm  frmfontsize txtDispatchDateTimeForPickupModal" readonly />
                        </div>
                        <div class="form-group col-sm-6">
                            <label for="txtDispatchEmployeeNameForPickupModal">Dispatched to</label>
                            <input type="text" id="txtDispatchEmployeeNameForPickupModal" class="form-control form-control-sm  frmfontsize txtDispatchEmployeeNameForPickupModal" readonly />
                        </div>
                    </div>

                    <div class="row">
                        <div class="form-group col-sm-6">
                            <label for="txtPickupDateTime">Pickup date and time</label>
                            <input type="datetime-local" id="txtPickupDateTime" class="form-control form-control-sm  frmfontsize txtPickupDateTime" />
                        </div>
                        <div class="form-group col-sm-6">
                            <label for="txtPickupWaitTime">Wait time (hour)</label>
                            <input type="number" id="txtPickupWaitTime" class="form-control form-control-sm frmfontsize txtPickupWaitTime" placeholder="Wait time" min="0" step=".01" title="Wait time (in hours)" />
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-sm btn-primary pl-5 pr-5" id="btnSavePickup"><i class="fa fa-save"></i> SAVE</button>
                    <button type="button" class="btn btn-sm btn-secondary" id="btnRemovePickup"><i class="fa fa-trash"></i> UNDO PICKUP </button>
                </div>
            </form>
        </div>
    </div>
</div>

<div class="modal fade" id="orderPassOn" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content bg-light">
            <div class="modal-header">
                <label class="modal-title">PASS-ON</label>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form class="" style="text-transform:uppercase; font-weight:600">
                <div class="modal-body">
                    <div class="row mb-3">
                        <div class="form-group col-sm-2">
                            <label for="txtWayBillNoForPassOnModal">W/B #</label>
                            <input class="form-control form-control-sm frmfontsize" placeholder="W/B #" id="txtWayBillNoForPassOnModal" type="number" name="txtWayBillNoForPassOnModal" readonly>
                        </div>
                    </div>

                    <div class="row mb-3">
                        <div class="form-group col-sm-4">
                            <label for="txtDispatchDateTimeForPassOnModal">DISPATCH DATE</label>
                            <input type="datetime-local" id="txtDispatchDateTimeForPassOnModal" class="form-control form-control-sm frmfontsize txtDispatchDateTimeForPassOnModal" readonly />
                        </div>
                        <div class="form-group col-sm-4">
                            <label for="txtPickupDateTimeForPassOnModal">PICKUP DATE</label>
                            <input type="datetime-local" id="txtPickupDateTimeForPassOnModal" class="form-control form-control-sm frmfontsize txtPickupDateTimeForPassOnModal" readonly />
                        </div>
                        <div class="form-group col-sm-4">
                            <label for="txtDispatchEmployeeNameForPassOnModal">DISPATCHED TO</label>
                            <input type="text" id="txtDispatchEmployeeNameForPassOnModal" class="form-control form-control-sm frmfontsize txtDispatchEmployeeNameForPassOnModal" readonly />
                        </div>

                    </div>
                    <div class="row ">
                        <div class="form-group col-sm-4">
                            <label for="txtPassOnDateTime">PASS-ON DATE</label>
                            <input type="datetime-local" id="txtPassOnDateTime" class="form-control form-control-sm frmfontsize txtPassOnDateTime" />
                        </div>
                        <div class="form-group col-sm-4">
                            <label for="txtPassOnEmployeeName">PASS-ON TO</label>
                            <div class="row mb-1 ">
                                <div class="col-sm-12 input-group">
                                    <input type="search" id="txtPassOnEmployeeName" name="txtPassOnEmployeeName" class="form-control form-control-sm frmfontsize txtPassOnEmployeeName" placeholder="Employee name" list="dlPassOnEmployees" />
                                    <datalist id="dlPassOnEmployees">
                                        @foreach (var employee in Model.Employees)
                                        {
                                            <option data-employeeid="@employee.Id" value="@employee.FirstName"></option>
                                        }
                                    </datalist>
                                </div>
                            </div>
                        </div>


                        <div class="col-sm-3 form-group">
                            <label for="txtPassOnWaitTime">WAIT TIME (HR)</label>
                            <input type="number" id="txtPassOnWaitTime" class="form-control form-control-sm frmfontsize txtPassOnWaitTime" placeholder="Wait time" min="0" step=".01" title="Wait time (in hours)" />
                        </div>


                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-sm btn-primary pl-5 pr-5" id="btnSavePassOn"><i class="fa fa-save"></i> SAVE</button>
                    <button type="button" class="btn btn-sm btn-secondary" id="btnRemovePassOn"><i class="fa fa-trash"></i> UNDO PASS-ON</button>
                </div>
            </form>
        </div>
    </div>
</div>

<div class="modal fade" id="orderDeliver" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content bg-light">
            <div class="modal-header">
                <label class="modal-title">ORDER DELIVERY</label>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form class="" style="text-transform:uppercase; font-weight:600">
                <div class="modal-body">
                    <div class="row mb-4">
                        <div class="form-group col-sm-2">
                            <label for="txtWayBillNoForDeliverModal">W/B number</label>
                            <input class="form-control form-control-sm frmfontsize" placeholder="W/B #" id="txtWayBillNoForDeliverModal" type="number" name="txtWayBillNoForDeliverModal" readonly>
                        </div>
                        <div class="form-group col-sm-5">
                            <label for="lblShipperInfo">Shipper</label>
