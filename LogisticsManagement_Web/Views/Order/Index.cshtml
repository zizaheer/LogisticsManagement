                            <label for="txtDeliveryNote">Notes</label>
                            <textarea id="txtDeliveryNote" class="form-control form-control-sm frmfontsize txtDeliveryNote" placeholder="Delivery note" title="Delivery note" rows="1"></textarea>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-sm-6">
                            <label for="txtDeliveryDateTime">Delivery Date</label>
                            <input type="datetime-local" id="txtDeliveryDateTime" class="form-control form-control-sm frmfontsize txtDeliveryDateTime" />
                        </div>
                        <div class="form-group col-sm-6">
                            <label for="txtDeliveryWaitTime">Wait time (hour)</label>
                            <input type="number" id="txtDeliveryWaitTime" class="form-control form-control-sm frmfontsize txtDeliveryWaitTime" placeholder="Wait time" min="0" step=".01" title="Wait time (in hours)" />
                        </div>
                    </div>

                    <!--<div class="row mb-3">
                            <div class="form-group col-sm-12">
                                <label for="txtReceivedBySign">Signature </label>
                                <canvas id="signatureCanvas" class="card" height="80"></canvas>
                                <span style="float:left">
                                    <button type="button" class="btn btn-sm btn-secondary" id="btnOk"><i class="fa fa-check"></i> Accept </button>
                                    <button type="button" class="btn btn-sm btn-secondary" id="btnCancel"><i class="fa fa-times"></i> Erase</button>
                                    <input type="hidden" id="imgSignature" />
                                </span>
                            </div>
                        </div> -->
                </div>
                <div class="modal-footer">
                    <div class="form-check-inline mr-5">
                        <input class="form-check-input" type="checkbox" id="chkPrintWaybill" checked />
                        <label class="form-check-label"> Print WB </label>
                    </div>
                    <button type="button" class="btn btn-sm btn-primary pl-5 pr-5" id="btnSaveDeliver"><i class="fa fa-save"></i> SAVE</button>
                    <button type="button" class="btn btn-sm btn-secondary" data-dismiss="modal" id="btnClose"><i class="fa fa-trash"></i> CLOSE</button>
                </div>
            </form>
        </div>
    </div>
</div>

<div class="modal fade" id="dispatchOrder" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content bg-light">
            <div class="modal-header">
                <label class="modal-title">DISPATCH ORDER</label>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form class="" style="text-transform:uppercase; font-weight:600">
                <div class="modal-body">
                    <div class="row mb-3">
                        <div class="form-group col-sm-4">
                            <label class="mr-4">WB #</label>
                            <label id="lblOrderNumbers" style="color:#c70101; font-size:1.1em"></label>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="form-group col-sm-6">
                            <label for="txtEmployeeName">Select employee/driver</label>
                            <input type="search" id="txtEmployeeName" name="txtEmployeeName" class="form-control form-control-sm frmfontsize txtEmployeeName" placeholder="Select employee/driver" list="dlEmployees" />
                            <datalist id="dlEmployees">
                                @foreach (var employee in Model.Employees)
                                {
                                    <option data-employeeid="@employee.Id" value="(@employee.Id) @employee.FirstName"></option>
                                }
                            </datalist>
                        </div>
                        <div class="form-group col-sm-4" style="display:none">
                            <label for="txtDispatchDatetimeForNewOrders">Dispatch date</label>
                            <input class="form-control form-control-sm  frmfontsize" id="txtDispatchDatetimeForNewOrders" type="datetime-local" min="1" name="txtDispatchDatetimeForNewOrders">
                        </div>
                        <div class="form-group col-sm-6">
                            <label for="txtOrderPortionForNewOrders">Agreed rate</label>
                            <div class="input-group">
                                <select class=" col-sm-5 form-control form-control-sm" id="ddlShareType"><option value="0">Fixed</option><option value="1">Pct.(%)</option></select>
                                <input class="form-control form-control-sm  frmfontsize" id="txtOrderPortionForNewOrders" type="number" name="txtOrderPortionForNewOrders" placeholder="Agreed rate">
                            </div>
                        </div>
                    </div>
                    <div class="row mb-3" style="display:none">
                        <div class="form-group col-sm-12">
                            <label for="txtVehicleName">Select vehicle</label>
                            <input class="form-control form-control-sm frmfontsize" placeholder="Vehicle ref #" id="txtVehicleName" type="search" name="txtVehicleName" list="dlVehicles">
                            <datalist id="dlVehicles"></datalist>
                        </div>
                    </div>

                    <div class="row">
                        <div class="form-group col-sm-12">
                            <label for="txtEmailAddressForDispatch">Email address</label>
                            <div class="input-group">
                                <input class="form-control form-control-sm frmfontsize" id="txtEmailAddressForDispatch" type="text" name="txtEmailAddressForDispatch" placeholder="Email address">
                            </div>
                        </div>
                    </div>


                </div>
                <div class="modal-footer">
                    <div class="form-check-inline mr-4">
                        <input type="checkbox" id="chkSendEmail" class="form-check-input" />
                        <small><b><label class="form-check-label" for="chkSendEmail"> Send email</label></b></small>
                    </div>
                    <button type="button" class="btn btn-sm btn-primary" id="btnDispatchToEmployee" title="Dispatch this order"><i class="fa fa-arrow-circle-right"></i> DISPATCH THIS ORDER</button>
                    <button type="button" class="btn btn-sm btn-secondary" id="btnClose"> CLOSE</button>
                </div>
            </form>
        </div>
    </div>
</div>

